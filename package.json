{
  "name": "typecast-ai",
  "version": "0.1.0",
  "description": "The smallest, most reliable way to get structured data out of any LLM. Contract-first with Zod.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm",
    "structured-output",
    "zod",
    "openai",
    "anthropic",
    "ollama"
  ],
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0"
  },
  "overrides": {
    "node-domexception": "^2.0.2",
    "glob": "^11.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "openai": "^4.73.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "engines": {
    "node": ">=22"
  }
}
